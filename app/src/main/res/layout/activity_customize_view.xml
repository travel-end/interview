<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical">
    <TextView
        android:id="@+id/testSwipeCaptchaView"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="自定义view-仿斗鱼滑动验证码" />
    <TextView
        android:id="@+id/testSpiderNetView"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="自定义view-五彩蛛网" />
    <TextView
        android:id="@+id/testCornersGifView"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="自定义view-圆角gifView" />

    <TextView
        android:id="@+id/testRandomDragView"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="自定义view-拖拽标签view" />
</LinearLayout>